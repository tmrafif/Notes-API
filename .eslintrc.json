{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "quotes": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "comma-dangle": "off"
    }
}
